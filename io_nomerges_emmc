!model
class=io
!view
view=title
text=(存储I/O)调试帮助
@show
echo true
@show
!view

!view
view=subtitle
text=禁用可提高性能
@show
echo true
@show
!view

!view
view=switch
text=启用
@show
if [ -e /sys/block/mmcblk0/queue/nomerges ]
then
echo true
fi
@show
core=true
@current
if [ `cat /sys/block/mmcblk0/queue/nomerges` == 1 ]
then
echo true
fi
@current
@click
echo $switch > /sys/block/mmcblk0/queue/nomerges
@click
root=true
!view
!model
